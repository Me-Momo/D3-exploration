{
  "name": "d3-exploration",
  "version": "1.0.0",
  "description": "拥有实时重载，并且支持es6语法的脚手架",
  "main": ".eslintrc.js",
  "scripts": {
    "start": "gulp",
    "watch": "tsc watch",
    "test": "jest"
  },
  "keywords": [
    "gulp",
    "babel"
  ],
  "author": "kasmine",
  "license": "MIT",
  "devDependencies": {
    "@types/debug": "0.0.30",
    "@types/node": "^8.0.28",
    "babel-preset-es2015": "^6.24.1",
    "debug": "^3.0.1",
    "gulp": "^3.9.1",
    "gulp-connect": "^5.0.0",
    "gulp-less": "^3.3.0",
    "gulp-rename": "^1.2.2",
    "gulp-uglify": "^3.0.0",
    "minimist": "^1.2.0",
    "pump": "^1.0.2",
    "rollup": "^0.49.3",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-typescript2": "^0.5.2",
    "ts-jest": "^21.0.1",
    "typescript": "^2.5.2"
  },
  "dependencies": {
    "@types/d3": "^4.10.0",
    "@types/debug": "0.0.30",
    "@types/gulp-babel": "^6.1.29",
    "@types/typescript": "^2.0.0",
    "debug": "^3.0.1",
    "path": "^0.12.7",
    "typescript": "^2.5.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Me-Momo/D3-exploration.git"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/src/.*/.*\\.spec\\.(js|ts|tsx)$",
    "collectCoverageFrom": [
      "src/**/*.{js,ts,tsx}",
      "!**/*.d.ts"
    ],
    "coverageDirectory": "coverage",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test_configs/.file-mock.js",
      "\\.(css|less)$": "<rootDir>/test_configs/.jest-ignore.js"
    },
    "moduleDirectories": [
      "node_modules"
    ],
    "setupFiles": [
      "<rootDir>/tests/setup.js"
    ]
  }
}
