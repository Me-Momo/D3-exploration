{
  "compilerOptions": {
    "typeRoots" : [ "node_modules" ],
    "types" : [ "types.d3" ],
    "lib": [
      "es6",
      "dom"
    ],
    "target": "es5",
    "moduleResolution": "node",
    "strict": true,
    "module": "es2015",
    "allowJs": false,
    "baseUrl": "src",
    "rootDir": "src",
    "outDir": "dist",
    "noUnusedLocals": true,
    "noImplicitAny": false
    },
    "exclude": [
      "dist",
      "src/scripts",
      "node_modules"
    ]
}